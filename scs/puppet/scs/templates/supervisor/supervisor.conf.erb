[supervisord]
directory = /scs
logfile = /scs/var/log/supervisord/supervisord.log
nodaemon = true
pidfile = /scs/var/run/supervisord/supervisord.pid

[inet_http_server]
port = *:9001

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl = http://localhost:9001

[include]
files = /scs/etc/supervisor.d/*.conf
